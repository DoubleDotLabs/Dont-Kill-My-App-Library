<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DokiContentView">
        <attr name="dokiPrimaryTextColor" format="color|reference"/>
        <attr name="dokiSecondaryTextColor" format="color|reference"/>
        <attr name="dokiButtonsTextColor" format="color|reference"/>
        <attr name="dokiActiveIconsColor" format="color|reference"/>
        <attr name="dokiInactiveIconsColor" format="color|reference"/>
        <attr name="dokiActiveIconsDrawable" format="reference"/>
        <attr name="dokiInactiveIconsDrawable" format="reference"/>
        <attr name="dokiExplanationTitle" format="string|reference"/>
        <attr name="dokiSolutionTitle" format="string|reference"/>
        <attr name="dokiBackgroundColor" format="color|reference"/>
        <attr name="dokiDividerColor" format="color|reference"/>
        <attr name="dokiImgBorderColor" format="color|reference"/>
        <attr name="dokiHeaderBackgroundColor" format="color|reference"/>
        <attr name="dokiIconsStyle" format="enum">
            <enum name="THUMB" value="0"/>
            <enum name="FACE" value="1"/>
            <enum name="POOP" value="2"/>
            <enum name="TRASH" value="3"/>
        </attr>
    </declare-styleable>
</resources>
